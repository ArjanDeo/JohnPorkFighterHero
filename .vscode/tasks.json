{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build JohnPorkFighterHero",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "${workspaceFolder}/out/build/Ninja-GCC"
